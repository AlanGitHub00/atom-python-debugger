{
  "name": "python-debugger",
  "main": "./lib/python-debugger",
  "version": "0.0.1",
  "description": "Develop and Debug python projects",
  "keywords": [
    "python",
    "develop python",
    "debug python",
    "pdb"
  ],
  "activationCommands": {
    "atom-workspace": [
      "python-debugger:toggle",
      "python-debugger:breakpoint"
    ]
  },
  "repository": "https://github.com/dpo/atom-python-debugger",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.0"
  },
  "activationHooks": [
    "language-python:grammar-used"
  ]
}
